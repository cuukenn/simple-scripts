root = true

[*]
charset = utf-8
end_of_line = lf

[*.{groovy,java,kt,xml}]
indent_style = tab
indent_size = 4

[*.java]
ij_java_class_count_to_use_import_on_demand = 999999999

[*.md]
insert_final_newline = false
trim_trailing_whitespace = false

[*.bat]
end_of_line = crlf

[*.cmd]
end_of_line = crlf
